<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:color/holo_green_dark">

    <fragment android:name="dk.cphbusiness.template.ObjFragment"
        android:id="@+id/objFragment"
        android:layout_width="match_parent"
        android:layout_height="380dp" />

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="380dp"
        tools:context="dk.cphbusiness.template.MapFragment" />

    <Button
        android:id="@+id/backBtn"
        style="@style/button_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/red_button"
        android:text="Back" />

    <Button
        android:id="@+id/objBtn"
        style="@style/button_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/red_button"
        android:text="Objectives"
        android:layout_above="@+id/backBtn"
        android:layout_alignLeft="@+id/backBtn"
        android:layout_alignStart="@+id/backBtn" />

</RelativeLayout>